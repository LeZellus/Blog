<form method="post" action="{{ path('log_delete', {'id': log.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ log.id) }}">
    <button class=""><i class="ph-trash text-red-500 text-2xl"></i></button>
</form>
